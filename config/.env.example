# AWS PostgreSQL Environment Configuration
# Copy this to .env and update with your AWS RDS details

# AWS RDS PostgreSQL Connection Details
AWS_DB_HOST=your-rds-instance.abc123.us-east-1.rds.amazonaws.com
AWS_DB_PORT=5432
AWS_DB_NAME=national_products
AWS_DB_USER=your_username
AWS_DB_PASSWORD=your_password

# Optional: AWS Credentials (if using IAM authentication)
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1

# Application Settings
ENVIRONMENT=production
LOG_LEVEL=INFO
BATCH_SIZE=1000

# Crawler Settings  
CRAWLER_RATE_LIMIT=1.0
MAX_RETRIES=3
TIMEOUT=30

# Example AWS RDS Endpoint format:
# myinstance.abc123.us-east-1.rds.amazonaws.com
